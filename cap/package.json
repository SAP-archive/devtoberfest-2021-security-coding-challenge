{
    "name": "cap",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^5.5.4",
        "@sap/cds-dk": "^4.5.4",
        "@sap/cds-odata-v2-adapter-proxy": "^1.7.13",
        "express": "^4",
        "hdb": "^0.19",
        "helmet": "^4.6.0",
        "passport": "^0.5.0",
        "path": "^0.12.7",
        "swagger-ui-express": "^4.1.6",
        "util": "^0.12.4"
    },
    "devDependencies": {
        "sqlite3": "^5.0.2"
    },
    "scripts": {
        "start": "cds run"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "requires": {
            "db": {
                "kind": "sql"
            },
            "auth": {
                "kind": "basic-auth",
                "users": {
                    "devtober": {
                        "password": "1234",
                        "roles": [
                            "DevtoberManager"
                        ]
                    }
                }
            }
        },
        "hana": {
            "deploy-format": "hdbtable"
        }
    }
}
